{
	"liveSassCompile.settings.formats": [
		{
			"format": "expanded",
			"extensionName": ".css",
			"savePath": "/styles"
		}
	],
	"liveSassCompile.settings.excludeList": [
		"/**/node_modules/**",
		"/.vscode/**"
	],
	"liveSassCompile.settings.generateMap": false,
	"liveSassCompile.settings.autoprefix": [
		"> 1%",
		"last 2 versions"
	],
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#75c7d2",
		"activityBar.background": "#75c7d2",
		"activityBar.foreground": "#15202b",
		"activityBar.inactiveForeground": "#15202b99",
		"activityBarBadge.background": "#c03fb1",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#15202b99",
		"sash.hoverBorder": "#75c7d2",
		"statusBar.background": "#4eb7c6",
		"statusBar.foreground": "#15202b",
		"statusBarItem.hoverBackground": "#379caa",
		"statusBarItem.remoteBackground": "#4eb7c6",
		"statusBarItem.remoteForeground": "#15202b",
		"titleBar.activeBackground": "#4eb7c6",
		"titleBar.activeForeground": "#15202b",
		"titleBar.inactiveBackground": "#4eb7c699",
		"titleBar.inactiveForeground": "#15202b99"
	},
	"peacock.color": "#4eb7c6",
